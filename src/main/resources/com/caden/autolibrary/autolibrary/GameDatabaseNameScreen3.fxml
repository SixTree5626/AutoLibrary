<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.caden.autolibrary.autolibrary.Controller">
    <children>
        <Label text="Name:" />
        <TextField fx:id="tfName" alignment="CENTER" onAction="#switchToScene2" prefWidth="150.0" promptText="Enter your name" />
        <Button fx:id="okBtn" text="Ok" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
